{
  "name": "muv",
  "version": "0.0.1",
  "description": "mooven component lib",
  "main": "dist/muv.min.js",
  "scripts": {
    "start": "npm run storybook",
    "lint": "eslint --fix src",
    "build": "webpack --progress",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "jest": {
    "setupFiles": [
      "./.jest/setup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ],
    "silent": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mooven-dev/muv.git"
  },
  "keywords": [
    "react",
    "component",
    "lib"
  ],
  "author": "clayton-duarte, drd-2rcl, igorassis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mooven-dev/muv/issues"
  },
  "homepage": "https://github.com/mooven-dev/muv#readme",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.0-rc.0",
    "@storybook/addon-info": "^4.0.0-rc.0",
    "@storybook/addon-links": "^4.0.0-rc.0",
    "@storybook/addons": "^4.0.0-rc.0",
    "@storybook/react": "^4.0.0-rc.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-docgen": "^2.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.5",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.5.2",
    "styled-components": "^3.4.10",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-ver-plugin": "0.0.2"
  }
}
